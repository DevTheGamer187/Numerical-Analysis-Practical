Cell[BoxData[
 RowBox[{
  RowBox[{"RK4", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"{", 
     RowBox[{"x0_", ",", "xn_"}], "}"}], ",", "y0_", ",", "steps_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"xold", "=", "x0"}], ",", 
      RowBox[{"yold", "=", "y0"}], ",", 
      RowBox[{"sollist", "=", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"x0", ",", "y0"}], "}"}], "}"}]}], ",", "x", ",", "y", ",", 
      "n", ",", "h", ",", "k1", ",", "k2", ",", "k3", ",", "k4", ",", "fn", 
      ",", "ynew", ",", "xnew"}], "}"}], ",", 
    RowBox[{
     RowBox[{"h", "=", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"xn", "-", "x0"}], ")"}], "/", "steps"}], "]"}]}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"xnew", "=", 
         RowBox[{"xold", "+", "h"}]}], ";", 
        RowBox[{"k1", "=", 
         RowBox[{"h", "*", 
          RowBox[{"f", "[", 
           RowBox[{"xold", ",", "yold"}], "]"}]}]}], ";", 
        RowBox[{"k2", "=", 
         RowBox[{"h", "*", 
          RowBox[{"f", "[", 
           RowBox[{
            RowBox[{"xold", "+", 
             FractionBox["h", "2"]}], ",", 
            RowBox[{"yold", "+", 
             FractionBox["k1", "2"]}]}], "]"}]}]}], ";", 
        RowBox[{"k3", "=", 
         RowBox[{"h", "*", 
          RowBox[{"f", "[", 
           RowBox[{
            RowBox[{"xold", "+", 
             FractionBox["h", "2"]}], ",", 
            RowBox[{"yold", "+", 
             FractionBox["k2", "2"]}]}], "]"}]}]}], ";", 
        RowBox[{"k4", "=", 
         RowBox[{"h", "*", 
          RowBox[{"f", "[", 
           RowBox[{
            RowBox[{"xold", "+", "h"}], ",", 
            RowBox[{"yold", "+", "k3"}]}], "]"}]}]}], ";", 
        RowBox[{"ynew", "=", 
         RowBox[{"yold", "+", 
          RowBox[{
           FractionBox["1", "6"], "*", 
           RowBox[{"(", 
            RowBox[{"k1", "+", 
             RowBox[{"2", "k2"}], "+", 
             RowBox[{"2", "k3"}], "+", "k4"}], ")"}]}]}]}], ";", 
        RowBox[{"sollist", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"sollist", ",", 
           RowBox[{"{", 
            RowBox[{"xnew", ",", "ynew"}], "}"}]}], "]"}]}], ";", 
        RowBox[{"xold", "=", "xnew"}], ";", 
        RowBox[{"yold", "=", "ynew"}]}], ",", 
       RowBox[{"{", "steps", "}"}]}], "]"}], ";", 
     RowBox[{"Return", "[", "sollist", "]"}]}]}], "]"}]}]], "Input",
