Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", "17"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
   RowBox[{"PlotStyle", "->", "Red"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"a", "=", "2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "a", "]"}], "*", 
      RowBox[{"f", "[", "b", "]"}]}], ">", "0"}], ",", 
    RowBox[{
    "Print", "[", "\"\<We cannot continue with the Bisection Method.\>\"", 
     "]"}], ",", 
    RowBox[{
     RowBox[{"i", "=", "1"}], ";", 
     RowBox[{"p", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
     RowBox[{"OutputDetails", "=", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"i", ",", "a", ",", "p", ",", "b", ",", 
         RowBox[{"f", "[", "p", "]"}]}], "}"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "<", "n"}], ",", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "a", "]"}], "]"}], "==", 
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "p", "]"}], "]"}]}], ",", 
          RowBox[{"a", "=", "p"}], ",", 
          RowBox[{"b", "=", "p"}]}], "]"}], ";", 
        RowBox[{"p", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
        RowBox[{"i", "++"}], ";", 
        RowBox[{"OutputDetails", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"OutputDetails", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "a", ",", "p", ",", "b", ",", 
             RowBox[{"f", "[", "p", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
      "]"}]}]}], "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Combining", " ", "the", " ", "output", " ", "details", " ", "with", " ", 
    "the", " ", "headings", " ", "of", " ", "the", " ", "table"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Print", "[", 
    RowBox[{"NumberForm", "[", 
     RowBox[{
      RowBox[{"N", "[", 
       RowBox[{"TableForm", "[", 
        RowBox[{"OutputDetails", ",", 
         RowBox[{"TableHeadings", "->", 
          RowBox[{"{", 
