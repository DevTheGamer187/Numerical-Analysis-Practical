Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"xi", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "1", ",", "3"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fi", "=", 
   RowBox[{"{", 
    RowBox[{"1", ",", "3", ",", "55"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", 
   RowBox[{"Length", "[", "xi", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"m", "=", 
   RowBox[{"Length", "[", "fi", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"n", "!=", "m"}], ",", 
    RowBox[{
    "Print", "[", 
     "\"\<List of points and function values are not of same size\>\"", "]"}],
     ",", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "n"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"L", "[", 
          RowBox[{"i", ",", "x_"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            UnderoverscriptBox["\[Product]", 
             RowBox[{"j", "=", "1"}], 
             RowBox[{"i", "-", "1"}]], 
            FractionBox[
             RowBox[{"x", "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], 
             RowBox[{
              RowBox[{"xi", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            UnderoverscriptBox["\[Product]", 
             RowBox[{"j", "=", 
              RowBox[{"i", "+", "1"}]}], "n"], 
            FractionBox[
             RowBox[{"x", "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], 
             RowBox[{
              RowBox[{"xi", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}]}]}], ";"}]}], "]"}],
      ";", 
     RowBox[{
      RowBox[{"Polynomial", "[", "x_", "]"}], "=", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "n"], 
       RowBox[{
        RowBox[{"L", "[", 
         RowBox[{"k", ",", "x"}], "]"}], "*", 
        RowBox[{"fi", "[", 
         RowBox[{"[", "k", "]"}], "]"}]}]}]}], ";"}]}], "]"}], 
  " "}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<Langrange Polynomial = \>\"", ",", 
   RowBox[{"Simplify", "[", 
    RowBox[{"Polynomial", "[", "x", "]"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Polynomial", "[", "2", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"A", "=", 
