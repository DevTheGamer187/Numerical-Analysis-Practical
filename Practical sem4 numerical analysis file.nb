e9d783a54666"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"eular", "[", 
    RowBox[{"f_", ",", 
     RowBox[{"{", 
      RowBox[{"x_", ",", "x0_", ",", "xn_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"y_", ",", "y0_"}], "}"}], ",", "steps_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"xold", "=", "x0"}], ",", 
       RowBox[{"yold", "=", "y0"}], ",", 
       RowBox[{"sollist", "=", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"x0", ",", "y0"}], "}"}], "}"}]}], ",", "x", ",", "y", ",", 
       "h"}], "}"}], ",", 
     RowBox[{
      RowBox[{"h", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"xn", "-", "x0"}], ")"}], "/", "steps"}], "]"}]}], ";", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"xnew", "=", 
          RowBox[{"xold", "+", "h"}]}], ";", 
         RowBox[{"ynew", "=", 
          RowBox[{"yold", "+", 
           RowBox[{"h", "*", 
            RowBox[{"(", 
             RowBox[{"f", "/.", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"x", "->", "xold"}], ",", 
                RowBox[{"y", "->", "yold"}]}], "}"}]}], ")"}]}]}]}], ";", 
         RowBox[{"sollist", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"sollist", ",", 
            RowBox[{"{", 
             RowBox[{"xnew", ",", "ynew"}], "}"}]}], "]"}]}], ";", 
         RowBox[{"xold", "=", "xnew"}], ";", 
         RowBox[{"yold", "=", "ynew"}]}], ",", 
        RowBox[{"{", "steps", "}"}]}], "]"}], ";", 
      RowBox[{"Return", "[", "sollist", "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
