Cell[TextData[{
 StyleBox["Ques.1 :- Perform 10 iterations of Bisection Method to find the \
root of the function f(x) = ",
  FontColor->RGBColor[0.5, 0, 0.5],
  Background->RGBColor[1, 0.85, 0.85]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "3"], "-", 
    RowBox[{"5", "x"}], "+", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,
  FontColor->RGBColor[0.5, 0, 0.5],
  Background->RGBColor[1, 0.85, 0.85],ExpressionUUID->
  "c10b38a6-92d8-4f9b-8f77-8c3dfa0133db"],
 StyleBox[" in the interval [0,1]",
  FontColor->RGBColor[0.5, 0, 0.5],
  Background->RGBColor[1, 0.85, 0.85]]
}], "Text",
 CellChangeTimes->{{3.9162728296416*^9, 
  3.9162729152593107`*^9}},ExpressionUUID->"4098b46c-ade7-40e8-b260-\
7d0d499a5c64"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{
    SuperscriptBox["x", "3"], "-", 
    RowBox[{"5", "x"}], "+", "1"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
   RowBox[{"PlotStyle", "->", "Red"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"a", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "a", "]"}], "*", 
      RowBox[{"f", "[", "b", "]"}]}], ">", "0"}], ",", 
    RowBox[{
    "Print", "[", "\"\<We cannot continue with the Bisection Method.\>\"", 
     "]"}], ",", 
    RowBox[{
     RowBox[{"i", "=", "1"}], ";", 
     RowBox[{"p", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
     RowBox[{"OutputDetails", "=", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"i", ",", "a", ",", "p", ",", "b", ",", 
         RowBox[{"f", "[", "p", "]"}]}], "}"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "<", "n"}], ",", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "a", "]"}], "]"}], "==", 
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "p", "]"}], "]"}]}], ",", 
          RowBox[{"a", "=", "p"}], ",", 
          RowBox[{"b", "=", "p"}]}], "]"}], ";", 
        RowBox[{"p", "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}], "/", "2"}]}], ";", 
        RowBox[{"i", "++"}], ";", 
        RowBox[{"OutputDetails", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"OutputDetails", ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "a", ",", "p", ",", "b", ",", 
             RowBox[{"f", "[", "p", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
      "]"}]}]}], "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Combining", " ", "the", " ", "output", " ", "details", " ", "with", " ", 
    "the", " ", "headings", " ", "of", " ", "the", " ", "table"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Print", "[", 
    RowBox[{"NumberForm", "[", 
     RowBox[{
      RowBox[{"N", "[", 
       RowBox[{"TableForm", "[", 
        RowBox[{"OutputDetails", ",", 
         RowBox[{"TableHeadings", "->", 
          RowBox[{"{", 
           RowBox[{"None", ",", 
            RowBox[{"{", 
             RowBox[{
             "\"\<i\>\"", ",", "\"\<\!\(\*SubscriptBox[\(a\), \(i\)]\)\>\"", 
              ",", "\"\<\!\(\*SubscriptBox[\(p\), \(i\)]\)\>\"", ",", 
              "\"\<\!\(\*SubscriptBox[StyleBox[\"b\",FontWeight->\"Plain\"], \
\"i\"]\)\>\"", ",", "\"\<f[\!\(\*SubscriptBox[\(p\), \(i\)]\)]\>\""}], 
             "}"}]}], "}"}]}]}], "]"}], "]"}], ",", "8"}], "]"}], "]"}], 
   ";"}], 
  RowBox[{"(*", 
   RowBox[{"Printing", " ", "Table"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<Root After \>\"", ",", "n", ",", "\"\< iterations \>\"", ",", 
   RowBox[{"NumberForm", "[", 
    RowBox[{
     RowBox[{"N", "[", "p", "]"}], ",", "8"}], "]"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "8", " ", "is", " ", "used", " ", "to", " ", "give", " ", "the", " ", 
    "result", " ", "with", " ", "8", " ", "digits", " ", "precision"}], 
   "*)"}]}]}], "Input",
